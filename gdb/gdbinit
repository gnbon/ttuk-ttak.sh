define init-peda
	source ~/gdb/peda/peda.py
end
document init-peda
	echo "Initializes the PEDA (Python Exploit Development Assistant for GDB) framework"
end

define init-pwndbg
	source ~/gdb/pwndbg/gdbinit.py
end
document init-pwndbg
	Initializes PwnDBG
end

define init-gef
	source ~/gdb/gef/gef.py
end
document init-gef
	Initializes GEF (GDB Enhanced Features)
end
